   "source": [
    "# counting elements - O(n + m)\n",
    "# 리스트 A 의 모든 요소는 set {0, 1 ... m} 사이에 존재 \n",
    "\n",
    "def counting(A, m):\n",
    "    n = len(A)\n",
    "    count = [0] * (m + 1)\n",
    "    for i in range(n):\n",
    "        count[A[i]] += 1\n",
    "    return count \n",
    "\n",
    "counting([0,0,4,2,4,5], 5)"
   ]
  }
 ],
